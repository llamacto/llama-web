{
  "name": "llamafront-ai-scaffold",
  "version": "2.0.0",
  "description": "LlamaFront AI Scaffold - AI-powered frontend application scaffold built with Next.js, TypeScript, and Tailwind CSS for the AI era",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:custom": "next dev --turbopack --port $PORT --hostname $HOSTNAME",
    "build": "BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true next build",
    "build:quiet": "next build 2>/dev/null || next build",
    "start": "next start --port $PORT",
    "start:custom": "next start --port $PORT --hostname $HOSTNAME",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "clean": "rimraf .next .turbo .eslintcache *.tsbuildinfo coverage",
    "clean:all": "rimraf .next .turbo .eslintcache *.tsbuildinfo coverage node_modules",
    "reset": "pnpm clean:all && pnpm install"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "tailwindcss",
    "ai-scaffold",
    "vibe-coding",
    "ai-development",
    "frontend",
    "ui-components",
    "llamafront"
  ],
  "author": "Llamacto Team",
  "license": "MIT",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
